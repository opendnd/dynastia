{
  "name": "dynastia-gui",
  "version": "0.1.0",
  "main": "main.js",
  "dependencies": {
    "dynastia": "git://github.com/opendnd/dynastia.git",
    "electron": "^1.4.14",
    "html-pdf": "^2.1.0"
  },
  "scripts": {
    "start": "electron .",
    "debug": "DYNASTIA_DEBUG=true electron .",
    "releaseMac": "electron-packager . dynastia --platform=darwin --arch=x64 --icon=icons/icon.icns --overwrite",
    "releaseWin32": "electron-packager . dynastia --platform=win32 --arch=ia32 --icon=icons/icon.ico --overwrite",
    "releaseWin64": "electron-packager . dynastia --platform=win32 --arch=x64 --icon=icons/icon.ico --overwrite"
  }
}
